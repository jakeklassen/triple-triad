{
  "name": "@tripletriad/frontend-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:vite": "vite",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./src/tailwind.css",
    "build": "tsc && npm run generate:css && vite build",
    "preview": "vite preview",
    "start": "npx http-server --brotli -P http://127.0.0.1:${PORT} ./dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.1.1",
    "@tripletriad/game": "workspace:0.1.0",
    "clsx": "1.2.1",
    "just-random": "3.1.1",
    "just-shuffle": "4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "4.5.4",
    "type-fest": "3.2.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier": "^2.8.0",
    "tailwindcss": "3.2.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
}
